{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "news",
  "info": {
    "name": "动态资讯"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "内容": {
      "type": "richtext"
    },
    "类型": {
      "type": "enumeration",
      "enum": [
        "热点资讯",
        "产业动态"
      ],
      "default": "热点资讯"
    },
    "权重": {
      "type": "integer",
      "default": 0,
      "max": 100,
      "min": 0
    },
    "标题": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 1
    }
  }
}
